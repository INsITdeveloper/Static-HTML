{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "router/anime.js", "src": "/anime" },
    { "handle": "router/games.js", "src": "/games" },
    { "handle": "router/pinterest.js", "src": "/pinterest" },
    { "handle": "router/default.js", "src": "/(.*)" }
  ]
}
